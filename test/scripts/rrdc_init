PATH=/usr/local/sbin:/usr/sbin/:$PATH

if test -d test/scripts ; then TEST=test; else TEST=.; fi

python $TEST/scripts/rrdc_job.py > /tmp/rrdclog_$USER 2>&1 &
echo $! >> /tmp/rrdctest_$USER

