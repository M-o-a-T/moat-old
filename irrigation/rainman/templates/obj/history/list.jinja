{% haml %}

-extends "base.jinja"
-import "obj/history/macro.jinja" as h
-import "obj/macro.jinja" as m

-block title
	History

-block content
	%h1
		-if site is defined
			-set show_site=False
			History for {{ site.name }}
		-else
			-set show_site=True
			All History entries

	=h.table(history_list, show_site=show_site)

	=m.pager(page_obj)

{% endhaml %}
