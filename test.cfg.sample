config:
  testing: true
  etcd:
    host: localhost
    port: 2379
    root: /test/moat
  amqp:
    server:
      host: localhost
      login: guest
      password: guest
      virtualhost: /moat
  logging:
    version: 1
    loggers: {}
    root:
      handlers: [stderr]
      level: INFO
    handlers:
      logfile:
        class: logging.FileHandler
        filename: test.log
        level: DEBUG
        formatter: std
      stderr:
        class: logging.StreamHandler
        level: DEBUG
        formatter: std
        stream: ext://sys.stderr
    formatters:
      std:
        format: '%(levelname)s:%(name)s:%(message)s'
